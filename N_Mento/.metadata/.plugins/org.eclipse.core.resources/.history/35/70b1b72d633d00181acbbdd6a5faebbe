import java.util.*; // 라이브러리
public class Ex1 {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num1 = 1;
		int num2 = 2;
		for(int i=0;i<5;i++) {
			for(int j=3;j>0+i;i++) {
				System.out.print(" ");
			}
			for(int k=1;k<=2*i+1;k++) {
				if(i%2==0) {
					System.out.print(num1);
					num1+=2;
				}
				if(i%2==1) {
					System.out.print(num2);
					num2+=2;
				}
			}
			System.out.println();
		}
	}
}
